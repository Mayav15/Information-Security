    "import hashlib\n",
    "\n",
    "plain_text = 'MY NAME IS ARYAN'\n",
    "result = hashlib.md5(plain_text.encode('utf-8'))\n",
    "\n",
    "print(\"The byte equivalent of hash is : \",result.digest())\n",
    "print(\"The hex equivalent of hash is : \",result.hexdigest())"
